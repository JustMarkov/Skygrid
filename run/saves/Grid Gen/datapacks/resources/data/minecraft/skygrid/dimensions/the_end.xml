<?xml version="1.0" encoding="UTF-8" ?>
<dimension replace="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.w3schools.com"
           xsi:schemaLocation="https://www.w3schools.com ../../../schema.xsd">

    <distance x="5" y="5" z="5"/>

    <loot>
        <table id="end_city_treasure"/>
    </loot>

    <tag weight="10" id="#end_stones">
        <side probability="0.1" on="up">
            <block id="chorus_flower"/>
            <block weight="10" id="chorus_plant">
                <side on="up">
                    <block id="chorus_flower"/>
                </side>
            </block>
        </side>
    </tag>

    <block id="end_stone_bricks"/>

    <collection name="purpur">
        <cycle key="axis"/>
        <block id="purpur_block">
            <side on="down">
                <block id="end_rod">
                    <set key="facing" value="down"/>
                </block>
            </side>
        </block>
        <block id="purpur_pillar"/>
    </collection>

    <block weight="0.05" id="shulker_box"/>
    <tag weight="0.01" id="#chests/ender">
            </tag>

    <block weight="0.002" id="dragon_head">
        <cycle key="rotation"/>
    </block>


</dimension>