<?xml version="1.0" encoding="UTF-8" ?>
<dimension replace="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.w3schools.com"
           xsi:schemaLocation="https://www.w3schools.com https://www.dropbox.com/s/h43h0a613s339e1/schema.xsd?dl=1">

    <loot>
        <table mod="skygridgame" id="chests/basic"/>
    </loot>

    <collection weight="10">

        <block weight="10" id="netherrack">
            <side probability="0.08" on="up">
                <block weight="100" id="red_mushroom" />
                <block weight="100" id="brown_mushroom" />
                <block id="wither_rose" />
            </side>
        </block>

        <block weight="3" id="soul_sand">
            <side probability="0.06" on="up">
                <block id="nether_wart">
                    <cycle key="age" />
                </block>
            </side>
        </block>

        <fallback weight="0.5">
            <block id="bedrock" />
            <block id="gravel" />
        </fallback>
        <block id="magma_block" />

    </collection>

    <block id="glowstone" />

    <collection weight="0.3">
        <block weight="5" id="nether_wart_block" />
        <block id="red_mushroom_block" />
    </collection>

    <collection weight="0.05">
        <block id="nether_brick_slab">
            <set key="type" value="top" />
            <side on="up">
                <block id="brewing_stand" />
            </side>
        </block>
        <block id="smithing_table" />
    </collection>

    <collection>
        <block weight="5" id="nether_bricks">
            <side probability="0.1" on="up">
                <block id="wither_skeleton_skull">
                    <cycle key="rotation" />
                </block>
            </side>
        </block>
        <block id="red_nether_bricks" />
        <block mod="quark" id="charred_nether_bricks" />
        <block mod="quark" id="magma_bricks" />

        <collection>
            <block mod="quark" id="cut_soul_sandstone"/>
            <block mod="quark" id="chiseled_soul_sandstone"/>
            <block mod="quark" id="soul_sandstone"/>
            <block mod="quark" id="soul_sandstone_bricks"/>
        </collection>

    </collection>

    <collection weight="0.6">
        <block weight="0.05" id="bone_block" />
        <block id="nether_quartz_ore" />
        <block weight="0.01" id="gold_block" />
        <block weight="0.03" mod="quark" id="blaze_lantern" />
        <block weight="0.02" mod="quark" id="nether_wart_sack" />
        <block weight="0.01" mod="botania" id="blaze_block" />
    </collection>

    <collection name="loot" weight="0.1">
        <block id="chest" />
    </collection>


</dimension>