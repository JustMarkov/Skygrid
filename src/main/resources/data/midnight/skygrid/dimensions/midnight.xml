<?xml version="1.0" encoding="UTF-8" ?>
<dimension replace="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.w3schools.com" xsi:schemaLocation="https://www.w3schools.com ../../../skygrid/skygrid/schema.xsd">

    <loot>
        <table weight="5" mod="midnight" id="chest/hidden_cache" />
        <table mod="midnight" id="chest/shadowroot_guardtower" />
    </loot>

    <collection name="fluids" weight="3">
        <block mod="midnight" id="miasma" />
        <block mod="midnight" id="dark_water" weight="0.2" />
    </collection>

    <collection weight="10" name="wood">
        <tag mod="midnight" id="#logs" />
        <collection name="leaves">
            <block mod="midnight" id="shadowroot_leaves" />
            <block mod="midnight" id="dark_willow_leaves">
                <side on="down">
                    <block mod="midnight" id="hanging_dark_willow_leaves" />
                </side>
            </block>
        </collection>
        <tag mod="midnight" id="#planks" />
    </collection>

    <collection weight="10" name="shrooms">
        <tag mod="midnight" id="#fungi_stems" />
        <tag mod="midnight" id="#fungi_hats" />
    </collection>

    <collection name="resources" weight="0.5">
        <block mod="midnight" id="suavis">
            <set key="stage" value="3" />
        </block>
    </collection>

    <collection weight="100" name="groud">

        <block mod="midnight" id="dirt" weight="0.1" />
        <block mod="midnight" id="coarse_dirt" weight="0.1" />
        <block mod="midnight" id="mycelium" weight="0.1" />
        <block mod="midnight" id="miasma_surface" weight="0.1" />
        <block mod="midnight" id="deceitful_peat" weight="0.1" />
        <block mod="midnight" id="deceitful_mud" weight="0.1" />

        <block mod="midnight" id="nightstone">
            <side on="up" probability="0.3">
                <block mod="midnight" id="glob_fungus" />
                <reference weight="3" mod="midnight" id="shrooms" />
                <block weight="0.1" mod="midnight" id="stinger_eggs">
                    <cycle key="eggs" />
                </block>
            </side>
        </block>

        <block weight="0.1" mod="midnight" id="trenchstone" />

        <collection weight="0.06" name="crystals">
            <block mod="midnight" id="bloomcrystal_rock">
                <side on="up">
                    <block mod="midnight" id="bloomcrystal" />
                </side>
            </block>
            <block mod="midnight" id="rouxe_rock">
                <side on="up">
                    <block mod="midnight" id="rouxe" />
                </side>
            </block>
        </collection>

        <block mod="midnight" id="grass_block">
            <side probability="0.7" on="up">

                <collection name="saplings">
                    <block mod="midnight" id="dark_willow_sapling" />
                    <block mod="midnight" id="shadowroot_sapling" />
                </collection>

                <block mod="midnight" id="ghost_plant" />
                <block mod="midnight" id="violeaf" />
                <block weight="0.5" mod="midnight" id="fingered_grass" />
                <block weight="0.2" mod="midnight" id="crystal_flower" />
                <block weight="0.5" mod="midnight" id="tendril_weed" />

                <collection name="bushes" weight="0.1">
                    <block mod="midnight" id="unstable_bush_blue_bloomed" />
                    <block mod="midnight" id="unstable_bush_green_bloomed" />
                    <block mod="midnight" id="unstable_bush_lime_bloomed" />
                </collection>

                <block mod="midnight" id="lumen_bud" />
                <block weight="0.1" mod="midnight" id="tall_lumen_bud">
                    <set key="half" value="lower" />
                    <side on="up">
                        <block mod="midnight" id="tall_lumen_bud">
                            <set key="half" value="upper" />
                        </block>
                    </side>
                </block>

                <block weight="0.1" mod="midnight" id="crystalotus" />

                <reference weight="2" mod="midnight" id="shrooms" />

                <block mod="midnight" id="grass" />

                <block mod="midnight" id="tall_grass">
                    <set key="half" value="lower" />
                    <side on="up">
                        <block mod="midnight" id="tall_grass">
                            <set key="half" value="upper" />
                        </block>
                    </side>
                </block>

            </side>
        </block>

    </collection>

    <collection name="building" weight="5">

        <block mod="midnight" id="archaic_glass" />

        <collection name="workstations">
            <collection>
                <block mod="midnight" id="shadowroot_crafting_table" />
                <block mod="midnight" id="dark_willow_crafting_table" />
                <block mod="midnight" id="dead_wood_crafting_table" />
                <block mod="midnight" id="nightshroom_crafting_table" />
                <block mod="midnight" id="dewshroom_crafting_table" />
                <block mod="midnight" id="vividshroom_crafting_table" />
                <block mod="midnight" id="bogshroom_crafting_table" />
            </collection>

            <block mod="midnight" id="nightstone_furnace" />
        </collection>

        <collection weight="30" name="bricks">
            <block mod="midnight" id="trenchstone_bricks" />
            <block weight="5" mod="midnight" id="nightstone_bricks" />
            <block weight="5" mod="midnight" id="chiseled_nightstone_bricks" />
            <block mod="midnight" id="rockshroom" />
            <block mod="midnight" id="rockshroom_bricks" />
        </collection>

    </collection>

    <collection name="ores" weight="10">
        <collection>
            <block mod="midnight" id="dark_pearl_ore" />
            <block mod="midnight" id="tenebrum_ore" />
            <block mod="midnight" id="nagrilite_ore" />
            <block mod="midnight" id="ebonite_ore" />
            <block mod="midnight" id="archaic_ore" />
        </collection>
        <collection name="blocks" weight="0.03">
            <block mod="midnight" id="dark_pearl_block" />
            <block mod="midnight" id="tenebrum_block" />
            <block mod="midnight" id="nagrilite_block" />
            <block mod="midnight" id="ebonite_block" />
        </collection>
    </collection>

    <collection name="loot">
        <block mod="midnight" id="viridshroom_stem_cache" />
        <block mod="midnight" id="shadowroot_chest" />
        <block mod="midnight" id="dark_willow_chest" />
        <block mod="midnight" id="dead_wood_chest" />
        <block mod="midnight" id="bogshroom_chest" />
    </collection>

</dimension>