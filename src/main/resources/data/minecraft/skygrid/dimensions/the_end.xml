<?xml version="1.0" encoding="UTF-8" ?>
<dimension replace="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.w3schools.com" xsi:schemaLocation="https://www.w3schools.com ../../../skygrid/skygrid/schema.xsd">

    <distance x="5" y="5" z="5" />

    <loot>
        <table id="end_city_treasure" />
    </loot>

    <collection weight="10">
        <tag weight="10" id="#end_stones">
            <side probability="0.1" on="up">
                <block id="chorus_flower" />
                <block weight="10" id="chorus_plant">
                    <side on="up">
                        <block id="chorus_flower" />
                    </side>
                </block>
            </side>
        </tag>

        <collection>
            <collection weight="2">
                <side on="up" probability="0.5">
                    <block weight="5" mod="endergetic" id="poise_grass" />
                    <block weight="0.5" mod="endergetic" id="bolloom_bud" />
                    <block mod="endergetic" id="poise_grass_tall">
                        <set key="half" value="lower" />
                        <side on="up">
                            <block mod="endergetic" id="poise_grass_tall">
                                <set key="half" value="upper" />
                            </block>
                        </side>
                    </block>
                </side>
                <block mod="endergetic" id="poise_grass_block" />
                <block mod="endergetic" id="poismoss_eumus" />
            </collection>
            <block mod="endergetic" id="poise_cluster" />
            <block mod="endergetic" id="eumus" />
        </collection>

    </collection>

    <collection weight="0.2">
        <cycle key="axis" />
        <block weight="3" mod="endergetic" id="poise_planks" />
        <block weight="2" mod="endergetic" id="poise_stem" />
        <block mod="endergetic" id="poise_stem_glowing" />
    </collection>

    <block id="end_stone_bricks" />

    <collection name="purpur">
        <cycle key="axis" />
        <block id="purpur_block">
            <side on="down">
                <block id="end_rod">
                    <set key="facing" value="down" />
                </block>
            </side>
        </block>
        <block id="purpur_pillar" />

        <block mod="quark" weight="0.5" id="duskbound_block" />
        <block mod="quark" weight="0.2" id="duskbound_lantern" />
    </collection>

    <collection weight="0.5" name="building">
        <block weight="0.08" mod="endergetic" id="poise_bookshelf" />
        <collection name="biotite">
            <block mod="quark" id="biotite_block" />
            <block mod="quark" id="chiseled_biotite_block" />
            <block mod="quark" id="smooth_biotite_block" />
            <block mod="quark" id="biotite_pillar" />
        </collection>

        <collection name="eumus">
            <block mod="endergetic" id="eumus_bricks" />
            <block mod="endergetic" id="eumus_chiseled_bricks" />
        </collection>
    </collection>


    <collection weight="0.5" name="ores">
        <block weight="50" mod="quark" id="biotite_ore" />
        <block mod="quark" id="chorus_fruit_block" />
        <block mod="charm" id="ender_pearl_block" />
    </collection>

    <collection weight="0.05" name="loot">
        <block id="shulker_box" />
        <block mod="quark" id="purpur_chest" />
        <block mod="quark" id="poise_chest" />
    </collection>

    <tag weight="0.01" id="#chests/ender" />

    <block weight="0.002" id="dragon_head">
        <cycle key="rotation" />
    </block>


</dimension>