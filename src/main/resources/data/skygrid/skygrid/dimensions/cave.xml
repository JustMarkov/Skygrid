<?xml version="1.0" encoding="UTF-8" ?>
<dimension replace="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.w3schools.com"
           xsi:schemaLocation="https://www.w3schools.com ../../../skygrid/skygrid/schema.xsd">

    <create enable="false" daytime="night" skylight="false">
        <biome id="mountains"/>
    </create>

    <loot>
        <table weight="5" id="spawn_bonus_chest"/>
        <table weight="2" id="simple_dungeon"/>
        <table id="abandoned_mineshaft"/>
        <table id="desert_pyramid"/>
    </loot>

    <reference id="ores" weight="10" />

    <collection name="stone" weight="100">
        <tag id="#wg_stone">
            <except>
                <name pattern="brimstone" />
                <name pattern="permafrost" />
            </except>
        </tag>
        <tag id="#gravel"/>
        <block id="clay"/>
    </collection>

    <block id="lava" />

    <block id="obsidian"/>

</dimension>